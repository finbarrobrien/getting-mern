{
  "presets": [
    "es2015",
    "stage-0",
    "babel-preset-react"
  ],
  "plugins": [
    "babel-plugin-transform-object-rest-spread",
    ["babel-plugin-root-import", {
      "rootPathPrefix": "~"
    }],
  ],
  "env": {
    "test": {
      "presets": ["es2015", "react", "stage-0"],
      "plugins": [
        ["babel-plugin-root-import", {
          "rootPathPrefix": "~"
        }]
      ]
    }
  }
}
